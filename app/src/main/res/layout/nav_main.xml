<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    android:background="#222222"
    android:paddingLeft="@dimen/base_padding"
    android:paddingStart="@dimen/base_padding"
    android:paddingRight="@dimen/base_padding"
    android:paddingEnd="@dimen/base_padding"
    android:paddingTop="18dp">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="#767b7f"
        android:orientation="horizontal">
        <!--이용권-->
        <LinearLayout
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_height="fill_parent"
            android:layout_margin="0.33dp"
            android:paddingTop="5dp"
            android:paddingBottom="5dp"
            android:background="#222222">
            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/a008_gnb_charge"
                android:paddingLeft="13.33dp"
                android:layout_gravity="center_vertical"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/menu_charge"
                android:textSize="15.66dp"
                android:textColor="#fff"
                android:layout_gravity="center"
                android:paddingLeft="8.67dp"
                />
        </LinearLayout>
        <!--최근 본 영상-->
        <LinearLayout
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_height="wrap_content"
            android:layout_marginTop="0.33dp"
            android:layout_margin="0.33dp"
            android:paddingTop="5dp"
            android:paddingBottom="5dp"
            android:background="#222222">
            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/a008_logout"
                android:paddingLeft="13.33dp"
                android:layout_gravity="center_vertical"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/menu_last_watched"
                android:textSize="15.66dp"
                android:textColor="#fff"
                android:layout_gravity="center"
                android:paddingLeft="8.67dp"
                />
        </LinearLayout>
    </LinearLayout>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="#fff"
        android:text="@string/menu_my_info"
        android:paddingTop="18dp"
        android:paddingBottom="11.33dp"/>
    <!--내정보-->
    <android.support.constraint.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="#767b7f">
        <TableLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:stretchColumns="*"
            android:background="#333333">
            <TableRow
                android:layout_weight="1">
                <LinearLayout
                    android:id="@+id/view_menu_user_info"
                    android:layout_width="0dp"
                    android:layout_weight="1"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:paddingTop="6.67dp"
                    android:background="#222222"
                    android:layout_marginRight="0.33dp"
                    android:layout_marginEnd="0.33dp"
                    android:layout_marginLeft="0.33dp"
                    android:layout_marginStart="0.33dp"
                    android:layout_marginTop="0.33dp"
                    android:layout_marginBottom="0.33dp">
                    <ImageView
                        android:id="@+id/img_gnb_member"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:src="@drawable/a008_gnb_member"
                        android:layout_gravity="center_horizontal"/>
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:paddingTop="8.67dp"
                        android:paddingBottom="11.33dp"
                        android:gravity="center_horizontal"
                        android:text="회원정보"
                        android:textColor="#ffffff"
                        android:textSize="10.43dp" />
                </LinearLayout>
                <LinearLayout
                    android:id="@+id/view_menu_last_watched"
                    android:layout_width="0dp"
                    android:layout_weight="1"
                    android:layout_height="fill_parent"
                    android:orientation="vertical"
                    android:paddingTop="6.67dp"
                    android:layout_marginTop="0.33dp"
                    android:layout_marginRight="0.33dp"
                    android:layout_marginEnd="0.33dp"
                    android:layout_marginBottom="0.33dp"
                    android:background="#222222"
                    android:gravity="bottom">

                    <ImageView
                        android:id="@+id/img_gnb_watch"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_horizontal"
                        android:src="@drawable/a008_gnb_watch" />

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center_horizontal"
                        android:paddingBottom="11.33dp"
                        android:paddingTop="8.67dp"
                        android:text="시청영상"
                        android:textColor="#ffffff"
                        android:textSize="10.43dp" />
                </LinearLayout>
                <LinearLayout
                    android:id="@+id/view_menu_favorite"
                    android:layout_width="0dp"
                    android:layout_weight="1"
                    android:layout_height="fill_parent"
                    android:orientation="vertical"
                    android:layout_marginRight="0.33dp"
                    android:layout_marginEnd="0.33dp"
                    android:layout_marginTop="0.33dp"
                    android:layout_marginBottom="0.33dp"
                    android:background="#222222"
                    android:gravity="bottom">
                    <ImageView
                        android:id="@+id/img_gnb_hearth"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:src="@drawable/a008_gnb_heart"
                        android:layout_gravity="center_horizontal"/>

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:paddingTop="8.67dp"
                        android:paddingBottom="11.33dp"
                        android:gravity="center_horizontal"
                        android:text="찜한영상"
                        android:textColor="#ffffff"
                        android:textSize="10.43dp"
                        />
                </LinearLayout>
                <LinearLayout
                    android:id="@+id/view_menu_subscribe"
                    android:layout_width="0dp"
                    android:layout_weight="1"
                    android:layout_height="fill_parent"
                    android:orientation="vertical"
                    android:background="#222222"
                    android:layout_marginTop="0.33dp"
                    android:layout_marginRight="0.33dp"
                    android:layout_marginEnd="0.33dp"
                    android:layout_marginBottom="0.33dp"
                    android:gravity="bottom">
                    <ImageView
                        android:id="@+id/img_gnb_subscribe"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:src="@drawable/a008_gnb_subscribe"
                        android:layout_gravity="center_horizontal"/>

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center_horizontal"
                        android:paddingTop="8.67dp"
                        android:paddingBottom="11.33dp"
                        android:text="구독채널"
                        android:textColor="#ffffff"
                        android:textSize="10.43dp"
                        />
                </LinearLayout>
            </TableRow>
            <TableRow>
                <!--<View-->
                    <!--android:layout_width="match_parent"-->
                    <!--android:layout_height="0.33dp"-->
                    <!--android:background="#767b7f"/>-->
            </TableRow>
            <TableRow>
                <LinearLayout
                    android:id="@+id/view_menu_point_history"
                    android:layout_width="0dp"
                    android:layout_weight="1"
                    android:layout_height="fill_parent"
                    android:orientation="vertical"
                    android:layout_marginRight="0.33dp"
                    android:layout_marginEnd="0.33dp"
                    android:layout_marginBottom="0.33dp"
                    android:layout_marginLeft="0.33dp"
                    android:layout_marginStart="0.33dp"
                    android:gravity="bottom"
                    android:background="#222222">
                    <ImageView
                        android:id="@+id/img_gnb_history"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_horizontal"
                        android:src="@drawable/a008_gnb_history"/>

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center_horizontal"
                        android:paddingTop="8.67dp"
                        android:paddingBottom="11.33dp"
                        android:text="포인트내역"
                        android:textColor="#ffffff"
                        android:textSize="10.43dp" />
                </LinearLayout>
                <LinearLayout
                    android:id="@+id/view_menu_pay_history"
                    android:layout_width="0dp"
                    android:layout_weight="1"
                    android:layout_height="fill_parent"
                    android:orientation="vertical"
                    android:layout_marginRight="0.33dp"
                    android:layout_marginEnd="0.33dp"
                    android:layout_marginBottom="0.33dp"
                    android:gravity="bottom"
                    android:background="#222">
                    <ImageView
                        android:id="@+id/img_gnb_payment"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:src="@drawable/a008_gnb_payment"
                        android:layout_gravity="center_horizontal"/>

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center_horizontal"
                        android:paddingTop="8.67dp"
                        android:paddingBottom="11.33dp"
                        android:text="결제내역"
                        android:textColor="#ffffff"
                        android:textSize="10.43dp"
                        />
                </LinearLayout>
                <LinearLayout
                    android:id="@+id/view_menu_setting"
                    android:layout_width="0dp"
                    android:layout_weight="1"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:background="#222222"
                    android:paddingTop="6.67dp"
                    android:layout_marginRight="0.33dp"
                    android:layout_marginEnd="0.33dp"
                    android:layout_marginBottom="0.33dp"
                    android:gravity="bottom">
                    <ImageView
                        android:id="@+id/img_gnb_setting"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:src="@drawable/a008_gnb_setting"
                        android:layout_gravity="center_horizontal"/>

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:paddingTop="8.67dp"
                        android:paddingBottom="11.33dp"
                        android:text="환경설정"
                        android:textColor="#ffffff"
                        android:textSize="10.43dp"
                        android:gravity="center_horizontal"/>
                </LinearLayout>

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_weight="1"
                    android:layout_height="fill_parent"
                    android:orientation="vertical"
                    android:background="#222222">
                </LinearLayout>
            </TableRow>
        </TableLayout>
    </android.support.constraint.ConstraintLayout>
    <!--고객센터-->
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:paddingTop="17.33dp"
        android:paddingBottom="11.33dp"
        android:text="고객센터"
        android:textColor="#fff"/>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:background="#767b7f">
        <LinearLayout
            android:id="@+id/view_menu_notice"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_height="fill_parent"
            android:paddingTop="6.67dp"
            android:orientation="vertical"
            android:background="#222222"
            android:layout_marginLeft="0.33dp"
            android:layout_marginStart="0.33dp"
            android:layout_marginTop="0.33dp"
            android:layout_marginBottom="0.33dp"
            android:gravity="bottom">
            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/a008_gnb_notice"
                android:layout_gravity="center_horizontal"
                />
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center_horizontal"
                android:paddingTop="8.67dp"
                android:paddingBottom="11.33dp"
                android:text="공지사항"/>
        </LinearLayout>
        <LinearLayout
            android:id="@+id/view_menu_faq"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_height="fill_parent"
            android:paddingTop="6.67dp"
            android:orientation="vertical"
            android:background="#222222"
            android:layout_marginLeft="0.33dp"
            android:layout_marginStart="0.33dp"
            android:layout_marginTop="0.33dp"
            android:layout_marginBottom="0.33dp"
            android:gravity="bottom">
            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/a008_gnb_faq"
                android:layout_gravity="center_horizontal"/>
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center_horizontal"
                android:paddingTop="8.67dp"
                android:paddingBottom="11.33dp"
                android:text="FAQ"/>
        </LinearLayout>
        <LinearLayout
            android:id="@+id/view_menu_qna"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_height="fill_parent"
            android:paddingTop="6.67dp"
            android:orientation="vertical"
            android:background="#222222"
            android:layout_marginLeft="0.33dp"
            android:layout_marginStart="0.33dp"
            android:layout_marginTop="0.33dp"
            android:layout_marginBottom="0.33dp"
            android:gravity="bottom">
            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/a008_gnb_qna"
                android:layout_gravity="center_horizontal"/>
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center_horizontal"
                android:paddingTop="8.67dp"
                android:paddingBottom="11.33dp"
                android:text="1:1 Q&amp;A"/>
        </LinearLayout>
        <LinearLayout
            android:id="@+id/view_menu_policy"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_height="wrap_content"
            android:paddingTop="6.67dp"
            android:orientation="vertical"
            android:background="#222222"
            android:layout_marginLeft="0.33dp"
            android:layout_marginStart="0.33dp"
            android:layout_marginTop="0.33dp"
            android:layout_marginBottom="0.33dp"
            android:layout_marginRight="0.33dp"
            android:layout_marginEnd="0.33dp">
            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/a008_gnb_polic"
                android:layout_gravity="center_horizontal"/>
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center_horizontal"
                android:paddingTop="8.67dp"
                android:paddingBottom="11.33dp"
                android:text="이용약관"/>
        </LinearLayout>
    </LinearLayout>
    
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="21.33dp"
        android:background="#767b7f">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:background="#222222"
            android:layout_margin="0.33dp"
            android:paddingLeft="16.67dp"
            android:paddingTop="7.67dp"
            android:paddingBottom="7.67dp">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="고객센터"
                android:textColor="#fff"
                android:paddingRight="16.67dp"/>
            <View
                android:layout_width="0.33dp"
                android:layout_height="match_parent"
                android:background="#767b7f"
                />
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:paddingLeft="16.67dp"
                android:orientation="horizontal">
                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Tel"
                        android:textSize="12.18dp"
                        android:textColor="#767b7f"/>
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Mail"
                        android:textSize="12.18dp"
                        android:textColor="#767b7f"/>
                </LinearLayout>
                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:paddingLeft="7.67dp"
                    android:orientation="vertical">
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="02-6953-1020"
                        android:textSize="12.18dp"
                        android:textColor="#fff"/>
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="customer@minimon.com"
                        android:textSize="12.18dp"
                        android:textColor="#fff"/>
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
</LinearLayout>