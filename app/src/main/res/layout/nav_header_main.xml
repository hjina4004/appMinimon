                android:textSize="@dimen/menu_font_size_link"
                android:gravity="center"
                android:layout_centerVertical="true"
                />
            <TextView
                android:id="@+id/tv_menu_point2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/user_point"
                android:textColor="#fff"
                android:textSize="13.92dp"
                android:layout_centerVertical="true"
                android:layout_toRightOf="@+id/img_menu_point"
                android:layout_marginLeft="9dp"
                android:layout_marginStart="9dp"
                android:layout_marginRight="16dp"
                android:gravity="center_vertical"
                android:layout_marginEnd="16dp"/>
            <TextView
                android:id="@+id/tv_menu_point"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="15.66dp"
                android:textColor="@color/MainColor"
                android:text="@string/init_point"
                android:gravity="center_vertical"
                android:layout_toRightOf="@id/tv_menu_point2"
                android:layout_centerVertical="true"/>
        </RelativeLayout>
        <RelativeLayout
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical">
            <TextView
                android:id="@+id/img_menu_ticket"
                android:layout_width="24dp"
                android:layout_height="24dp"
                android:background="@drawable/shape_circle"
                android:text="@string/mark_fixed"
                android:textColor="@color/colorBaseText"
                android:textSize="@dimen/menu_font_size_link"
                android:gravity="center"
                android:layout_centerVertical="true"
                />
