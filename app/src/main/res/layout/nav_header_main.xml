                android:id="@+id/tv_menu_ticket"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/menu_charge"
                android:textColor="#fff"
                android:textSize="13.92dp"
                android:layout_centerVertical="true"
                android:layout_toRightOf="@+id/img_menu_ticket"
                android:layout_marginLeft="9dp"
                android:layout_marginStart="9dp"
                android:layout_marginRight="16dp"
                android:layout_marginEnd="16dp"
                android:gravity="center_vertical"/>
            <TextView
                android:id="@+id/tv_menu_charge"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="13.92dp"
                android:textColor="#fff"
                android:text="@string/init_point"
                android:gravity="center_vertical"
                android:layout_toRightOf="@id/tv_menu_ticket"
                android:layout_centerVertical="true"/>
        </RelativeLayout>
